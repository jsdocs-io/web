---
import type { AllExtractedDeclaration } from "@jsdocs-io/extractor";
import { declarationSignature } from "../../lib/declaration-signature";

type Props = {
	declaration: AllExtractedDeclaration;
};

const { declaration } = Astro.props;
const signatureHtml = declarationSignature(declaration);
---

<Fragment set:html={signatureHtml} />
