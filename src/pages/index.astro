---
import Test from "../components/Test.svelte";
import BaseLayout from "../layouts/BaseLayout.astro";

const title = "jsDocs.io";
const description =
	"jsDocs.io is an open source documentation host for Javascript and Typescript packages published on npm";
---

<BaseLayout {title} {description}>
	<Test msg={"foo"} client:load />
	<p>Logo and name</p>
	<p>Automatically generated documentation for typed npm packages.</p>
	<p>How to use</p>
	<p>Link to guide</p>
	<p>What is jsDocs.io</p>
</BaseLayout>
