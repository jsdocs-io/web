{
    "private": true,
    "name": "@jsdocs-io/web",
    "description": "jsDocs.io website",
    "author": "velut",
    "version": "0.1.0",
    "scripts": {
        "dev": "next dev",
        "dev:redis": "cross-env REDIS_URL=redis://localhost:6379 next dev",
        "build": "next build",
        "start": "next start",
        "lint": "tsdx lint src test",
        "lint:fix": "tsdx lint src test --fix",
        "test": "jest --runInBand",
        "test:cov": "jest --runInBand --coverage",
        "test:watch": "jest --runInBand --watchAll --coverage",
        "test:ci": "jest --ci --maxWorkers=2 --coverage",
        "pre-push": "yarn && yarn lint && yarn build && yarn test:cov"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "yarn lint",
            "pre-push": "yarn pre-push"
        }
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "arrowParens": "always"
    },
    "dependencies": {
        "@braintree/sanitize-url": "^4.1.1",
        "@jsdocs-io/package-analyzer": "^1.0.0",
        "@keyv/redis": "^2.1.2",
        "@microsoft/tsdoc": "^0.12.21",
        "anchor-js": "^4.2.2",
        "clean-deep": "^3.4.0",
        "keyv": "^4.0.3",
        "next": "9.5.3",
        "nprogress": "^0.2.0",
        "path-to-regexp": "^6.1.0",
        "pretty-bytes": "^5.4.1",
        "prism-react-renderer": "^1.1.1",
        "prismjs": "^1.21.0",
        "query-registry": "^1.2.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "semver": "^7.3.2",
        "short-time-ago": "^1.1.1",
        "validate-npm-package-name": "^3.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "11.0.0",
        "@commitlint/config-conventional": "11.0.0",
        "@tailwindcss/typography": "0.2.0",
        "@testing-library/jest-dom": "5.11.4",
        "@testing-library/react": "11.0.4",
        "@types/anchor-js": "4.2.0",
        "@types/keyv": "3.1.1",
        "@types/node": "14.11.1",
        "@types/nprogress": "0.2.0",
        "@types/react": "16.9.49",
        "@types/semver": "7.3.4",
        "@types/validate-npm-package-name": "3.0.0",
        "babel-jest": "26.3.0",
        "cross-env": "7.0.2",
        "eslint-plugin-prettier": "3.1.4",
        "husky": "4.3.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "26.4.2",
        "postcss-preset-env": "6.7.0",
        "prettier": "2.1.2",
        "react-test-renderer": "16.13.1",
        "stylelint-config-recommended": "3.0.0",
        "tailwindcss": "1.8.10",
        "tailwindcss-dark-mode": "1.1.6",
        "ts-jest": "26.3.0",
        "tsdx": "0.13.3",
        "typescript": "4.0.3"
    }
}
