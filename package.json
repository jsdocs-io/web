{
    "private": true,
    "name": "@jsdocs-io/web",
    "description": "jsDocs.io website",
    "author": "Edoardo Scibona (velut)",
    "version": "0.1.0",
    "license": "AGPL-3.0-or-later",
    "scripts": {
        "dev": "next dev",
        "build": "cross-env NODE_OPTIONS='--max-old-space-size=7168' next build",
        "build:check": "tsc --noEmit",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "test": "jest --runInBand",
        "test:cov": "jest --runInBand --coverage",
        "test:watch": "jest --runInBand --watchAll --coverage",
        "test:ci": "jest --ci --maxWorkers=2 --coverage",
        "pre-push": "yarn && yarn lint && yarn build && yarn test:cov",
        "generate:favicon": "yarn real-favicon generate faviconDescription.json faviconData.json public",
        "generate:prerendered-packages": "yarn ts-node --skip-project scripts/update-prerendered-packages.ts",
        "generate:robots-txt": "yarn ts-node --skip-project scripts/update-robots-txt.ts",
        "generate:sitemap": "yarn ts-node --skip-project scripts/update-sitemap.ts",
        "analyze": "cross-env ANALYZE=true next build",
        "prepare": "husky install"
    },
    "dependencies": {
        "@braintree/sanitize-url": "^6.0.0",
        "@jsdocs-io/extractor": "^0.3.0",
        "@microsoft/tsdoc": "^0.13.2",
        "anchor-js": "^4.3.1",
        "clean-deep": "^3.4.0",
        "filter-anything": "^3.0.2",
        "fs-extra": "^10.0.0",
        "minio": "^7.0.26",
        "new-github-issue-url": "^1.0.0",
        "next": "12.0.9",
        "nprogress": "^0.2.0",
        "path-to-regexp": "^6.2.0",
        "piscina": "^3.2.0",
        "pretty-bytes": "^5.6.0",
        "prism-react-renderer": "^1.2.1",
        "prismjs": "^1.27.0",
        "query-registry": "^2.2.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-hotkeys-hook": "3.4.4",
        "semver": "^7.3.5",
        "short-time-ago": "^2.0.0",
        "stream-json": "^1.7.3",
        "swr": "^1.2.0",
        "trim-newlines": "4.0.2",
        "use-dark-mode": "^2.3.1",
        "validate-npm-package-name": "^3.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "16.1.0",
        "@commitlint/config-conventional": "16.0.0",
        "@next/bundle-analyzer": "12.0.9",
        "@testing-library/jest-dom": "5.16.1",
        "@testing-library/react": "12.1.2",
        "@types/anchor-js": "4.2.1",
        "@types/fs-extra": "9.0.13",
        "@types/jest": "27.4.0",
        "@types/minio": "7.0.12",
        "@types/node": "17.0.24",
        "@types/nprogress": "0.2.0",
        "@types/proper-url-join": "2.1.1",
        "@types/react": "17.0.38",
        "@types/semver": "7.3.9",
        "@types/stream-json": "1.7.2",
        "@types/validate-npm-package-name": "3.0.3",
        "autoprefixer": "10.4.2",
        "babel-jest": "27.4.6",
        "cli-real-favicon": "0.0.8",
        "cross-env": "7.0.3",
        "eslint": "8.8.0",
        "eslint-config-next": "12.0.9",
        "eslint-plugin-prettier": "4.0.0",
        "got": "11.8.3",
        "husky": "7.0.4",
        "identity-obj-proxy": "3.0.0",
        "jest": "27.4.7",
        "license-webpack-plugin": "4.0.1",
        "postcss": "8.4.5",
        "prettier": "2.5.1",
        "proper-url-join": "2.1.1",
        "react-test-renderer": "17.0.2",
        "stylelint-config-recommended": "6.0.0",
        "tailwindcss": "3.0.18",
        "ts-dedent": "2.2.0",
        "ts-node": "10.4.0",
        "typescript": "4.5.5",
        "webpack-stats-plugin": "1.0.3",
        "webpack-visualizer-plugin2": "1.0.0",
        "xml-formatter": "2.6.0"
    }
}
