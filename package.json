{
  "private": true,
  "name": "@jsdocs-io/web",
  "description": "jsDocs.io website",
  "author": "Edoardo Scibona (velut)",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=7168' next build",
    "build:check": "tsc --noEmit",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --watchAll --coverage",
    "test:ci": "jest --ci --maxWorkers=2 --coverage",
    "pre-push": "pnpm i && pnpm lint && pnpm build && pnpm test:cov",
    "generate:favicon": "pnpm real-favicon generate faviconDescription.json faviconData.json public",
    "generate:prerendered-packages": "pnpm tsx scripts/update-prerendered-packages.ts",
    "generate:robots-txt": "pnpm tsx scripts/update-robots-txt.ts",
    "generate:sitemap": "pnpm tsx scripts/update-sitemap.ts",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.4",
    "@heroicons/react": "^2.0.18",
    "@jsdocs-io/extractor": "^0.4.0",
    "@microsoft/tsdoc": "^0.14.2",
    "anchor-js": "^4.3.1",
    "clean-deep": "^3.4.0",
    "clsx": "^1.2.1",
    "filter-anything": "^3.0.7",
    "fs-extra": "^11.1.1",
    "minio": "^7.1.2",
    "new-github-issue-url": "^1.0.0",
    "next": "14.2.3",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.1",
    "piscina": "^3.2.0",
    "pretty-bytes": "^6.1.1",
    "prism-react-renderer": "^1.3.5",
    "prismjs": "^1.29.0",
    "query-registry": "^2.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hotkeys-hook": "4.4.1",
    "semver": "^7.5.4",
    "short-time-ago": "^2.0.0",
    "stream-json": "^1.8.0",
    "swr": "^2.2.2",
    "trim-newlines": "4.0.2",
    "tsx": "^3.12.8",
    "use-dark-mode": "^2.3.1",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@next/bundle-analyzer": "13.4.7",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/anchor-js": "5.0.0",
    "@types/fs-extra": "11.0.3",
    "@types/jest": "29.5.11",
    "@types/node": "18.16.3",
    "@types/nprogress": "0.2.0",
    "@types/proper-url-join": "2.1.4",
    "@types/react": "18.2.7",
    "@types/semver": "7.3.13",
    "@types/stream-json": "1.7.3",
    "@types/validate-npm-package-name": "4.0.0",
    "autoprefixer": "10.4.16",
    "babel-jest": "29.7.0",
    "cli-real-favicon": "0.0.8",
    "cross-env": "7.0.3",
    "eslint": "8.39.0",
    "eslint-config-next": "14.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "got": "11.8.6",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "license-webpack-plugin": "4.0.2",
    "postcss": "8.4.31",
    "prettier": "3.2.5",
    "proper-url-join": "2.1.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.4.0",
    "ts-dedent": "2.2.0",
    "typescript": "5.0.4",
    "webpack-stats-plugin": "1.1.3",
    "webpack-visualizer-plugin2": "1.0.0",
    "xml-formatter": "3.4.1"
  }
}
